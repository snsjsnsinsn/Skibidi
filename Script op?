local player = game.Players.LocalPlayer
local astro = player:WaitForChild("Twisted: Astro")
local vee = player:WaitForChild("Twisted: Vee")
local shelly = player:WaitForChild("Twisted: Shelly")
local sprout = player:WaitForChild("Twisted: Sprout")

local clone = player.Modelo:Clone(astro, vee, sprout, shelly)
clone.Parent = workspace
clone:SetPrimaryPartCFrame(CFrame.new(posicao))

spawnObjeto(astro, Vector3.new(0, 5, 0))
spawnObjeto(vee, Vector3.new(10, 5, 0))
spawnObjeto(shelly, Vector3.new(20, 5, 0))
spawnObjeto(sprout, Vector3.new(30, 5, 0))

local modelIDs = {
    ["Twisted sprout"] = "4500",
    ["Twisted Vee"] = "4500",
    ["Twisted Astro"] = "5000",
    ["Twisted Shelly"] = "4025",
}
