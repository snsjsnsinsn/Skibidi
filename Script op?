local twistedList = {
    "TwistedAstro",
    "TwistedShelly",
    "TwistedSprout",
    "TwistedVee",
    "TwistedPebble"
}

local function spawnTwisted(name)
    for _, v in pairs(game:GetDescendants()) do
        if v:IsA("Model") and v.Name == name then
            local clone = v:Clone()
            clone.Parent = game.Workspace
            clone:SetPrimaryPartCFrame(CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(math.random(-10,10), 3, math.random(-10,10))))
            return
        end
    end
end

for _, twisted in ipairs(twistedList) do
    spawnTwisted(twisted)
end
