local twistedList = {
    "TwistedAstro",
    "TwistedShelly",
    "TwistedSprout",
    "TwistedVee",
    "TwistedPebble"
}

local spawnPosition = Vector3.new(0, 10, 0)

for i, twistedName in ipairs(twistedList) do
    local twisted = game.ReplicatedStorage:FindFirstChild(twistedName)
    if twisted then
        local clone = twisted:Clone()
        clone.Parent = game.Workspace
        clone:SetPrimaryPartCFrame(CFrame.new(spawnPosition + Vector3.new(i * 10, 0, 0)))
    end
end
